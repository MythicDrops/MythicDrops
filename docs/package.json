{
  "name": "docs",
  "description": "Documentation website for MythicDrops",
  "repository": "git@github.com:MythicDrops/MythicDrops.git",
  "author": "Richard Harrah <topplethenunnery@gmail.com>",
  "packageManager": "pnpm@7.19.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nr prefetch && nr dev-no-prefetch",
    "dev-no-prefetch": "vitepress --port 3333 --open",
    "build": "nr prefetch && nr build-no-prefetch",
    "build-no-prefetch": "vitepress build && node .vitepress/scripts/build-pwa.mjs && nr copy-dist",
    "copy-dist": "cpy dist .. --cwd=.vitepress",
    "serve": "vitepress serve",
    "preview-https": "pnpm run build && serve .vitepress/dist",
    "preview-https-no-prefetch": "pnpm run build-no-prefetch && serve .vitepress/dist",
    "prefetch": "esno .vitepress/scripts/fetch-avatars.ts"
  },
  "devDependencies": {
    "@antfu/ni": "^0.18.8",
    "@iconify-json/carbon": "^1.1.11",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.41",
    "@types/properties-reader": "^2.1.1",
    "@unocss/reset": "^0.47.4",
    "@vitejs/plugin-vue": "^3.2.0",
    "cpy-cli": "^4.2.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.0",
    "https-localhost": "^4.7.1",
    "ohmyfetch": "^0.4.21",
    "pathe": "^1.0.0",
    "prettier": "^2.8.0",
    "prettier-plugin-pkg": "^0.17.1",
    "properties-reader": "^2.2.0",
    "unocss": "^0.47.4",
    "unplugin-vue-components": "^0.22.11",
    "vite": "3.2.4",
    "vite-plugin-pwa": "0.13.3",
    "vitepress": "1.0.0-alpha.29",
    "workbox-window": "^6.5.4"
  },
  "dependencies": {
    "@vueuse/core": "^9.6.0",
    "jiti": "^1.16.0",
    "vue": "^3.2.45"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  }
}
